        <script_true><![CDATA[function onCondition() {
    (function executeRule(current, gForm) {
    // Get the current state of the task
    var state = gForm.getValue('state');

    // Check if the state is "In Progress" (adjust the value if it's numeric or has another label)
    if (state == 'in progress') {
        gForm.setSectionDisplay('Inspection Report', true); // Show the section
    } else {
        gForm.setSectionDisplay('Inspection Report', false); // Hide the section
    }
   })(current, g_form);
}]]></script_true>
